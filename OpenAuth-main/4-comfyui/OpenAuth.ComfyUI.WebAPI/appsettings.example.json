{
  "Logging": {
    "LogLevel": {
      "Default": "Error", // 完全禁用默认日志
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "None"
    }
  }, 
  "ConnectionStrings": {
    "DefaultDB": "Data Source=.;Initial Catalog=comfyui;User=sa;Password=123456;TrustServerCertificate=true"
  },
  "AppSetting": {
    // redis配置
    "RedisConfig": {
      // 连接
      "ConnectionString": "127.0.0.1:6379",
      // 数据库
      "Database": "1" 
    },
    // 上传路径
    "UploadConfig": {
      "Path": "wwwroot",
      "Exclude": [  ],
      //上传方法 oss,local
      "UploadType": "local",
      "UploadPath": "D:\\wwwroot\\files"
    },
    "OssConfig": {
      "accessKey": "?",
      "secretKey": "?",
      "host": "?",
      "bucket": "?",
      "endpoint": "oss-cn-hangzhou.aliyuncs.com"
    },
    "AuthConfig": {
      // token密钥
      "SecretKey": "xyFOBo",
      // 有效期 24小时 (86400秒)
      "ExpireTime": "1440"
    },
    "CorsUrlConfig": [ "http://localhost:3000", "http://localhost:5173" ],
    //alipay支付
    "AlipayConfig": {
      "appId": "?",
      "privateKey": "?",
      "publicKey": "?",
      "gatewayUrl": "https://openapi-sandbox.dl.alipaydev.com/gateway.do",
      "signType": "RSA2",
      "charset": "UTF-8",
      "notifyUrl": "http://localhost:5111/alipay/notice",
      "returnUrl": "http://localhost:3000/",
      "encryptKey": "?",
      "encryptType": "AES",
      "format": "json",
      "timeout": "30m",
      "version": "1.0",
      "sellerId": "?"
    },
    "ComfyUIConfig": {
      "url": "http://127.0.0.1:7860",
      "timeout": 10, // 分钟
      "retry": 3, // 次数
      "path": "comfyui", //文件路径
      "uploadImage": "/api/upload/image",
      "uploadMask":  "/api/upload/mask"
    },
    //百度翻译
    "BaiduTransformConfig": {
      "AppId": "?",
      "SecretKey": "?",
      "Url": "http://api.fanyi.baidu.com/api/trans/vip/translate"
    },
    "SMSConfig": {
      "Endpoint": "dysmsapi.aliyuncs.com",
      "Region": "cn-hangzhou",
      "accessKey": "?",
      "secretKey": "?",
      "Template": {
        "login": "?"
      }
    }
  }
}
