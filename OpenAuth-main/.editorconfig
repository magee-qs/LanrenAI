[*.cs]

# IDE0059: 不需要赋值
dotnet_diagnostic.IDE0059.severity = none

# CS0168: 声明了变量，但从未使用过
dotnet_diagnostic.CS0168.severity = silent

# CS8604: 引用类型参数可能为 null。
dotnet_diagnostic.CS8604.severity = silent

# IDE0044: 添加只读修饰符
dotnet_diagnostic.IDE0044.severity = silent

# CS8625: 无法将 null 字面量转换为非 null 的引用类型。
dotnet_diagnostic.CS8625.severity = silent

# CS8618: 在退出构造函数时，不可为 null 的字段必须包含非 null 值。请考虑添加 "required" 修饰符或声明为可为 null。
dotnet_diagnostic.CS8618.severity = silent

# IDE0120: 简化 LINQ 表达式
dotnet_diagnostic.IDE0120.severity = silent

# CS8603: 可能返回 null 引用。
dotnet_diagnostic.CS8603.severity = silent

# CS8602: 解引用可能出现空引用。
dotnet_diagnostic.CS8602.severity = silent

 
